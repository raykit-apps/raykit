{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build:package": {
      "dependsOn": ["^build:package"]
    },
    "build:extension": {
      "dependsOn": ["^build:extension"]
    },
    "build:all": {
      "dependsOn": ["^build:package", "^build:extension"]
    },
    "dev": {
      "dependsOn": ["^build"]
    }
  }
}
